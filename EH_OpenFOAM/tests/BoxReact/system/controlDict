FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     EHFoam;

startFrom       latestTime; //startTime; //latestTime

startTime       0;

stopAt          endTime;

endTime         0.002;

deltaT          0.001;

writeControl    adjustableRunTime;

writeInterval   4.0;

purgeWrite      0;

writeFormat     ascii;

writePrecision  10;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

adjustTimeStep  no;

maxCo           1.0;

maxDeltaT       1;

